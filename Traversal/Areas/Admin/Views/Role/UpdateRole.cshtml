
@model UpdateRoleVM
@{
    ViewData["Title"] = "UpdateRole";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Rol Güncelleme</h1>
<form method="post">
    <input type="hidden" asp-for="RoleID"/>
    <label>Rol Adı</label>
    <input type="text" class="form-control" asp-for="RoleName"/>
    <button class="btn btn-outline-warning">Rolü Güncelle</button>
</form>

